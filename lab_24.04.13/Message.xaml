<Window x:Class="lab_24._04._13.Message"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Message" Height="212" Width="365" WindowStyle="ToolWindow" WindowStartupLocation="CenterOwner" ResizeMode="NoResize" Closed="Window_Closed" ShowInTaskbar="False" Topmost="True" Loaded="Window_Loaded">
    <Grid>
        <Label Name="Count" Content="You connected only 1 puzzle(s). Your time:" HorizontalAlignment="Center" Margin="0,12,0,0" VerticalAlignment="Top"/>
        <Label Name="time" Content="00:00:00" HorizontalAlignment="Center" Margin="0,36,0,0" VerticalAlignment="Top"/>
        <Label Content="Give your name" HorizontalAlignment="Center" Margin="0,60,0,0" VerticalAlignment="Top"/>
        <TextBox Name="name" HorizontalAlignment="Center" Height="23" Margin="0,88,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="169"/>
        <Button Content="OK" HorizontalAlignment="Center" Margin="-112,128,0,0" VerticalAlignment="Top" Width="75" Height="28" Click="OK_Button_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Center" Margin="112,128,0,0" VerticalAlignment="Top" Width="75" Height="28" Click="Cancel_Button_Click"/>
    </Grid>
</Window>
